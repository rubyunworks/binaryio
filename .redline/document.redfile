---
rdoc:
  service  : RDoc
  output   : site/docs/api
  format   : newfish
  include  : ~
  exclude  : ~
  main     : ~
  extra    : ~
  active   : true

ri:
  service  : RI
  include  : ~
  exclude  : ~
  active   : false

dnote:
  service  : DNote
  loadpath : ~
  labels   : ~
  output   : ~
  active   : false

vclog:
  service  : VClog
  layout   : rel    # gnu
  typed    : false
  output   : ~
  active   : false

